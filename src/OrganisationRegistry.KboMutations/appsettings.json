{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Wegwijs": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Wegwijs": "Verbose",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Properties": {
      "Application": "Wegwijs - KboMutations"
    }
  },
  "KboMutations": {
    "LockTableName": "__DistributedLocks__",
    "LockLeasePeriodInMinutes": 5,
    "MaxRetryAttempts": 5,
    "BatchSize": 1000
  }
}
